{
  "name": "veracode-policy-app-action",
  "version": "1.0.0",
  "description": "Initiate Veracode Policy Scans",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "check-types": "tsc --noEmit",
    "build": "tsc",
    "pack": "ncc build src/index.ts -o dist && git commit -am 'debug' && git push",
    "test": "jest",
    "test:coverage": "jest --clearCache && jest --coverage",
    "lint": "eslint . 'src/**/*.ts'",
    "prettier": "prettier --write . 'src/**/*.ts'",
    "run-test": "npx ts-node test/test.ts"
  },
  "keywords": [
    "Veracode",
    "Policy",
    "Scan",
    "Security",
    "GitHub",
    "Actions"
  ],
  "author": "Vincent Deng <vincentdeng.it@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.3",
    "@types/sjcl": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@octokit/rest": "^20.0.2",
    "csv-parse": "^5.5.6",
    "sjcl": "^1.0.8"
  }
}
